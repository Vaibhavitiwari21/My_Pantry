{
    "version": 2,
    "builds": [
      {
        "src": "nextjs/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/nextjs/$1"
      }
    ]
  }
  